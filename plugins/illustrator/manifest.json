{
  "manifestVersion": 5,
  "id": "io.grin.illustrator.plugin",
  "name": "GRIN Exporter",
  "version": "0.1.0",
  "main": "src/main.js",
  "host": {
    "app": "ILST",
    "minVersion": "25.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "grin-panel",
      "label": "GRIN Export",
      "minimumSize": {
        "width": 300,
        "height": 260
      },
      "maximumSize": {
        "width": 640,
        "height": 900
      },
      "panel": {
        "main": "src/index.html"
      }
    }
  ]
}
